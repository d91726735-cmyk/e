{
    "compilerOptions": {
        "target": "ES2019",
        "moduleResolution": "node",
        "module": "commonjs",
        "allowSyntheticDefaultImports": true,
        "outDir": "./",
        "rootDir": "./",
        "strict": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "declaration": false
    },
    "include": ["*.ts"],
    "exclude": ["node_modules", "dist"]
}